<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Views.FightScene"
             xmlns:vm="clr-namespace:ViewModels"
             x:DataType="vm:FightSceneViewModel">
  <StackPanel Background="{DynamicResource ResourceKey=BasicCaveBackground}">
    <StackPanel HorizontalAlignment="Left" Margin="10,10">
      <Button Classes="Wood" Name="GoBackButton" Command="{Binding GoBackToMapCommand}">
        <Grid ColumnDefinitions="auto,*,auto"><Image/><Label Content="Go back"/><Image/></Grid>
      </Button>
    </StackPanel>
    <StackPanel  Name="CardHolder" Orientation="Horizontal" HorizontalAlignment="Center">
    </StackPanel>
  </StackPanel>

</UserControl>
