<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Views.AllCardsMenu"
             xmlns:vm="clr-namespace:ViewModels"
             x:DataType="vm:AllCardsMenuViewModel">
  <StackPanel Background="{DynamicResource ResourceKey=AllCardsBackground}">
    <StackPanel HorizontalAlignment="Left" Margin="10,10">
      <Button Command="{Binding GoBackToMapCommand}" Classes="Wood" Name="GoBackButton">
        <Grid ColumnDefinitions="auto,*,auto"><Image/><Label Content="Go back to Map"/><Image/></Grid>
      </Button>
    </StackPanel>
    
    <WrapPanel Name="CardHolder" HorizontalAlignment="Center" VerticalAlignment="Center" MaxWidth="1600" ItemSpacing="10" LineSpacing="20" Margin="0, 100, 0, 0">
    </WrapPanel>
  </StackPanel>
</UserControl>
