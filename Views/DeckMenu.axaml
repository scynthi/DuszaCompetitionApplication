<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Views.DeckMenu"
             xmlns:vm="clr-namespace:ViewModels"
             x:DataType="vm:DeckMenuViewModel">
  <StackPanel>
    <StackPanel HorizontalAlignment="Left" Margin="10,10">
      <Button Command="{Binding GoBackToMapCommand}" Classes="Wood" Name="GoBackButton">
        <Grid ColumnDefinitions="auto,*,auto"><Image/><Label Content="Go back to Map"/><Image/></Grid>
      </Button>
    </StackPanel>
    <Label>This will be the deck menu.</Label>
  </StackPanel>
</UserControl>
